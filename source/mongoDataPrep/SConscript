sourceFiles = Split("""
                       main.cpp
                    """)

Import('env')
mongoDataPrepEnv = env.Clone()

mongoDataPrepEnv.MergeFlags('-O6 -Wall -Werror')
mongoDataPrepEnv['CPPPATH'] = '#/source'

mongoDataPrep = mongoDataPrepEnv.Program(target = 'mongoDataPrep', source = sourceFiles)
mongoDataPrepInstall = mongoDataPrepEnv.Install('#/bin', mongoDataPrep)

mongoDataPrepEnv.Default(mongoDataPrep)
mongoDataPrepEnv.Default(mongoDataPrepInstall)
